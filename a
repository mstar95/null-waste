def parse(s: String) = s.split(",").map(_.toInt).toSeq
val program1 = parse(input1)

def run(program: Seq[Int], input: Int, it: Int = 0, outPut: List[Int] = Nil): String = {
  
  def immediate(int: i = 0) = program(it + i);
  def parameter(int: i = 0) = program(immediate(it))
  program(it).
  
  program(it) match {
    case "99" => "1"
    case x => x.head match {
      
      run(program, it + 1)
    }
  }
}
